            "source": [
                "def squares(n):\n",
                "    \"\"\"Compute the squares of numbers from 1 to n\"\"\"\n",
                "    \n",
                "    ### BEGIN SOLUTION\n",
                "    if n < 1:\n",
                "        raise ValueError(\"n must be greater than or equal to 1\")\n",
                "    return [i**2 for i in range(1, n+1)]\n",
                "    ### END SOLUTION"
            ]
        }
    ],
